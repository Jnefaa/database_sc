{% extends 'base.html.twig' %}

{% block title %}Etudiant{% endblock %}

{% block body %}
    <h1>Etudiant</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ etudiant.id }}</td>
            </tr>
            <tr>
                <th>Matricule</th>
                <td>{{ etudiant.Matricule }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ etudiant.Nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ etudiant.Prenom }}</td>
            </tr>
            <tr>
                <th>DateNais</th>
                <td>{{ etudiant.DateNais ? etudiant.DateNais|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ etudiant.Email }}</td>
            </tr>
            <tr>
                <th>Adress</th>
                <td>{{ etudiant.Adress }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('etudiant_index') }}">back to list</a>

    <a href="{{ path('etudiant_edit', {'id': etudiant.id}) }}">edit</a>

    {{ include('etudiant/_delete_form.html.twig') }}
{% endblock %}
